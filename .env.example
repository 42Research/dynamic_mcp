# Browser configuration
BROWSER_HEADLESS=false
BROWSER_TIMEOUT=300
BROWSER_WINDOW_SIZE=1920x1080
BROWSER_USE_DEFAULT_PROFILE=false
BROWSER_ENABLE_PASSWORD_MANAGER=true
BROWSER_AUTO_FILL_PASSWORDS=true

# Authentication timing
AUTH_WAIT_TIME=10
MANUAL_AUTH_TIMEOUT=180

# Authentication configuration
AUTH_CACHE_TTL=3600
AUTH_RETRY_ATTEMPTS=3

# Logging
LOG_LEVEL=INFO

# Crash dump and kernel paths
CRASH_DUMP_PATH=/var/crash
KERNEL_PATH=/boot

# Crash utility timeouts (in seconds)
CRASH_TIMEOUT=120
SESSION_INIT_TIMEOUT=180

# Maximum number of crash dumps to list
MAX_CRASH_DUMPS=10

# Dynamic reverse connection configuration
# Set to "true" to enable reverse connection to Dynamic service
ENABLE_REVERSE_CONNECTION=false

# Dynamic service URL (default: http://localhost:8787)
DYNAMIC_URL=http://localhost:8787

# Dynamic API key (optional, for authenticated connections)
DYNAMIC_API_KEY=

# Heartbeat interval in seconds (default: 15)
HEARTBEAT_INTERVAL=15
